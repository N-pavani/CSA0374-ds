#include<stdio.h>
int main()
{
	int i,number;
	unsigned long long fact=1;  
	printf("enter a number");
	scanf("%d",&number);
	if(number<0)
	 printf("error!negative  numbers does not exist");
	else
	{
		for(i=1;i<=number;i++)
		{
			fact=fact*i;
		}
	printf("factorial of %d=%d",number,fact);
	}
  return 0;
}
